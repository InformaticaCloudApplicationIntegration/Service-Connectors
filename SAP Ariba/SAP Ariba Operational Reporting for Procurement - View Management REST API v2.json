    "/metadata": {
      "get": {
        "description": "Lists all fields and filters that are available for a given document type",
        "tags": [
          "View Management"
        ],
        "operationId": "get fields and filters of given document type",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "realm",
            "in": "query",
            "required": true,
            "type": "string",
            "description": "Customer realm name"
          },
          {
            "name": "product",
            "in": "query",
            "required": true,
            "type": "string",
            "enum": [
              "buyer",
              "sourcing"
            ],
            "description": "Value of this field should be \"buyer\" or \"sourcing\""
          },
          {
            "name": "documentType",
            "in": "query",
            "required": false,
            "type": "string",
            "description": "specify the document type for which the fields and filters needs to be listed. If not provided, it lists all document types's fields and filters"
          },
          {
            "name": "select",
            "in": "query",
            "required": false,
            "type": "array",
            "items": {
              "type": "string",
              "enum": [
                "documentType",
                "filterFields",
                "selectFields"
              ],
              "default": ""
            },
            "description": "specify the respose fields (documentType, filterFields, selectFields) to be displayed. Default - display all fields."
          },
          {
            "name": "jsonSchema",
            "in": "query",
            "required": false,
            "type": "boolean",
            "default": false,
            "description": "view metadata in JSON schema format"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/GetAllViewsresponse"
              },
              "example": {
                "documentType": "Requisition",
                "filterFields": [
                  {
                    "name": "StatusString",
                    "type": "string",
                    "size": 50,
                    "allowedValues": null,
                    "precision": null,
                    "scale": null
                  },
                  {
                    "name": "TimeCreated",
                    "type": "date",
                    "size": 50,
                    "allowedValues": null,
                    "precision": null,
                    "scale": null
                  },
                  {
                    "name": "UniqueName",
                    "type": "string",
                    "size": 50,
                    "allowedValues": null,
                    "precision": null,
                    "scale": null
                  },
                  {
                    "name": "TimeUpdated",
                    "type": "date",
                    "size": null,
                    "allowedValues": null,
                    "precision": null,
                    "scale": null
                  }
                ],
                "selectFields": [
                  {
                    "name": "ApprovalRequests.State",
                    "type": "int",
                    "size": null,
                    "allowedValues": [
                      {
                        "key": 8,
                        "value": "Approved"
                      },
                      {
                        "key": 1,
                        "value": "NotYetActive"
                      },
                      {
                        "key": 2,
                        "value": "ActiveButNoActionYet"
                      },
                      {
                        "key": 4,
                        "value": "Denied"
                      },
                      {
                        "key": 16,
                        "value": "Rejected"
                      }
                    ],
                    "precision": null,
                    "scale": null
                  }
                ]
              }
            }
          },
          "403": {
            "$ref": "#/responses/Forbidden"
          },
          "404": {
            "$ref": "#/responses/Unauthorized"
          },
          "500": {
            "$ref": "#/responses/InternalServerError"
          }
        }
      }
    }
