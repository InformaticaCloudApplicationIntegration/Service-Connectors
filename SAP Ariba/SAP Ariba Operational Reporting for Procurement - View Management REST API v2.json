        "op": {
          "description": "Operations allowed. <= >= == <> IN LIKE NOT LIKE",
          "type": "string"
        },
        "defaultValue": {
          "description": "Passing defaultvalue is optional. Default value can always be overridden.",
          "type": "string"
        }
